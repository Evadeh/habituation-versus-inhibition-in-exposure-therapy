@misc{Coffey2015-20180828,
  author = {Coffey SF and Banducci AN and Vinci C},
  title = {Common Questions About Cognitive Behavior Therapy for Psychiatric Disorders.},
  journal = {American family physician},
  journal.abbreviated = {Am Fam Physician},
