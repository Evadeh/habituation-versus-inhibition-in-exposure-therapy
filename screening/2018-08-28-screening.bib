@misc{McLeod2018a-20180828,
  author = {McLeod BD and Southam-Gerow MA and Rodriguez A and Quinoy AM and Arnold CC and Kendall PC and Weisz JR},
  title = {Development and Initial Psychometrics for a Therapist Competence Instrument for CBT for Youth Anxiety.},
  journal = {Journal of clinical child and adolescent psychology : the official journal for the Society of Clinical Child and Adolescent Psychology, American Psychological Association, Division 53},
