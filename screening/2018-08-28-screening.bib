  doi = {10.1007/s12310-012-9080-2},
  U1 = {Sponsor: National Institute of Mental Health, US. Grant: MH90027497. Recipients: Ginsburg, Golda S., Sponsor: National Institute of Mental Health, US. Grant: T32 MH 18834. Recipients: Becker, Emily M.},
