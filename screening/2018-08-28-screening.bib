  author = {Peris, Tara S. and Compton, Scott N. and Kendall, Philip C. and Birmaher, Boris and Sherrill, Joel and March, John and Gosch, Elizabeth and Ginsburg, Golda and Rynn, Moira and McCracken, James T. and Keeton, Courtney P. and Sakolsky, Dara and Suveg, Cynthia and Aschenbrand, Sasha and Almirall, Daniel and Iyengar, Satish and Walkup, John T. and Albano, Anne Marie and Piacentini, John},
  title = {Trajectories of change in youth anxiety during cognitive—behavior therapy.},
  journal = {Journal of Consulting and Clinical Psychology},
