}

@article{McGuire2016a-20180828,
  id.record = {2016-04202-001},
  number.accession = {2016-04202-001},
  author = {McGuire, Joseph F. and Orr, Scott P. and Wu, Monica S. and Lewin, Adam B. and Small, Brent J. and Phares, Vicky and Murphy, Tanya K. and Wilhelm, Sabine and Pine, Daniel S. and Geller, Daniel and Storch, Eric A.},
  title = {Fear conditioning and extinction in youth with obsessive-compulsive disorder.},
  journal = {Depression and Anxiety},
  JO = {Depression and Anxiety},
  journal.abbreviated = {Depress Anxiety},
  year = {2016/03//},
  volume = {33},
