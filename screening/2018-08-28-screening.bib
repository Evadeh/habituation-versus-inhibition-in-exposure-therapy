  author = {Jakic, Manda},
  title = {Kognitivno-bihevioralni pristup lijecenju opsesivno-kompulzivnog poremecaja kod djece. = Cognitive-behavioural approach to the treatment of obsessive-compulsive disorder in children.},
  journal = {Socijalna Psihijatrija},
  JO = {Socijalna Psihijatrija},
  year = {2017/06//},
  volume = {45},
