@misc{Peris2015a-20180828,
  author = {Peris TS and Compton SN and Kendall PC and Birmaher B and Sherrill J and March J and Gosch E and Ginsburg G and Rynn M and McCracken JT and Keeton CP and Sakolsky D and Suveg C and Aschenbrand S and Almirall D and Iyengar S and Walkup JT and Albano AM and Piacentini J},
