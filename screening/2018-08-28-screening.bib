  doi = {10.1007/978-1-4614-7624-5_26},
  U1 = {Sponsor: National Institute of Mental Health, US. Grant: R01MH081881. Recipients: Warner, Carrie Masia},
  ED = {Weist, Mark D., Lever, Nancy A., Bradshaw, Catherine P., Sarno Owens, Julie, Weist, Mark D., (Ed), Lever, Nancy A., (Ed), Bradshaw, Catherine P., (Ed), Sarno Owens, Julie, (Ed)},
