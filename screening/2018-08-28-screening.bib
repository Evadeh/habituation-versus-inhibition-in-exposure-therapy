  address.author = {Dorsey, Shannon, Department of Psychology, University of Washington, Guthrie Hall, Box 351525, Seattle, WA, US, 98195},
  doi = {10.1080/15374416.2016.1220309},
  U1 = {Sponsor: K01 MH092526; R01 MH103291, US. Grant: R01MH095749; R01MH96633. Recipients: Dorsey, Shannon (Prin Inv), Sponsor: National Institute of Mental Health, US. Grant: K01 MH092526; R01 MH103291. Recipients: McLaughlin, Katie A. (Prin Inv)},
  recordLog = {PsycINFO},
}

