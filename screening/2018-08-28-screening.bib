  database.name = {psyh},
  ER = {},
  U1 = {Sponsor: National Institute of Mental Health, US. Grant: K23 MH073946. Recipients: Ehrenreich-May, Jill},
  title.tertairy = {Programs that work},
  recordLog = {PsycINFO},
}

