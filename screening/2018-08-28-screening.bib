@misc{Pine2002a-20180828,
  author = {Pine DS and Cohen JA},
  title = {Trauma in children and adolescents: risk and treatment of psychiatric sequelae.},
  journal = {Biological psychiatry},
  journal.abbreviated = {Biol Psychiatry},
