  address.author = {Schneider, Silvia, Klinische Kinder- und Jugendpsychologie, Institut fur Psychologie, Universitat Basel, Missionsstrasse 60/62, CH-4055, Basel, Switzerland},
  doi = {10.1024/1422-4917.34.3.191},
