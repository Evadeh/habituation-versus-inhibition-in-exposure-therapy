@misc{Chu2015a-20180828,
  author = {Chu BC and Crocco ST and Arnold CC and Brown R and Southam-Gerow MA and Weisz JR},
  title = {Sustained Implementation of Cognitive-Behavioral Therapy for Youth Anxiety and Depression: Long-term Effects of Structured Training and Consultation on Therapist Practice in the Field.},
