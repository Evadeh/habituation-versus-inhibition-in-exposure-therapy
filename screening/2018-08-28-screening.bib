}

@article{Santacana2016-20180828,
  id.record = {2016-42195-001},
  number.accession = {2016-42195-001},
  author = {Santacana, Martí and Arias, Bárbara and Mitjans, Marina and Bonillo, Albert and Montoro, María and Rosado, Sílvia and Guillamat, Roser and Vallès, Vicenç and Pérez, Víctor and Forero, Carlos G. and Fullana, Miquel A.},
  title = {Predicting response trajectories during cognitive-behavioural therapy for panic disorder: No association with the BDNF gene or childhood maltreatment.},
  journal = {PLoS ONE},
  JO = {PLoS ONE},
  journal.abbreviated = {PLoS One},
  year = {2016/06/29/},
  volume = {11},
  issue = {6},
  place.published = {US},
  publisher = {Public Library of Science},
  number.serial = {1932-6203},
  notes = {Accession Number: 2016-42195-001. Partial author list: First Author & Affiliation: Santacana, Martí; Department of Mental Health, Consorci Sanitari de Terrassa, Terrassa, Spain. Release Date: 20170720. Publication Type: Journal (0100), Peer Reviewed Journal (0110). Format Covered: Electronic. Document Type: Journal Article. Language: English. Grant Information: Fullana, Miquel A. Major Descriptor: Child Abuse; Exposure Therapy; Genes; Panic Disorder; Polymorphism. Minor Descriptor: Cognitive Behavior Therapy; Treatment Outcomes. Classification: Cognitive Therapy (3311). Population: Human (10); Male (30); Female (40). Location: Spain. Age Group: Adulthood (18 yrs & older) (300). Tests & Measures: Panic Disorder Severity Scale-Spanish Self-Report Version; Mini International Neuropsychiatric Interview-Spanish Version; Childhood Trauma Questionnaire-Short Form--Spanish Version DOI: 10.1037/t25412-000. Methodology: Empirical Study; Interview; Quantitative Study; Treatment Outcome. Supplemental Data: Tables and Figures Internet. ArtID: e0158224. Issue Publication Date: Jun 29, 2016. Publication History: First Posted Date: Jun 29, 2016; Accepted Date: Jun 13, 2016; First Submitted Date: Mar 4, 2016. Copyright Statement: This is an open access article distributed under the terms of the Creative Commons Attribution License, which permits unrestricted use, distribution, and reproduction in any medium, provided the original author and source are credited. Santacana et al. 2016.},
  abstract = {Background: Anxiety disorders are highly prevalent and result in low quality of life and a high social and economic cost. The efficacy of cognitive-behavioural therapy (CBT) for anxiety disorders is well established, but a substantial proportion of patients do not respond to this treatment. Understanding which genetic and environmental factors are responsible for this differential response to treatment is a key step towards 'personalized medicine'. Based on previous research, our objective was to test whether the BDNF Val66Met polymorphism and/or childhood maltreatment are associated with response trajectories during exposure-based CBT for panic disorder (PD). Method: We used Growth Mixture Modeling to identify latent classes of change (response trajectories) in patients with PD (N = 97) who underwent group manualized exposure-based CBT.We conducted logistic regression to investigate the effect on these trajectories of the BDNF Val66Met polymorphism and two different types of childhood maltreatment, abuse and neglect. Results: We identified two response trajectories ('high response' and 'low response'), and found that they were not significantly associated with either the genetic (BDNF Val66Met polymorphism) or childhood trauma-related variables of interest, nor with an interaction between these variables. Conclusions: We found no evidence to support an effect of the BDNF gene or childhood trauma-related variables on CBT outcome in PD. Future studies in this field may benefit from looking at other genotypes or using different (e.g. whole-genome) approaches. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
  keywords = {BDNF Val66Met polymorphism, childhood maltreatment, response trajectories, exposure-based cognitive behavioral therapy, panic disorder, Child Abuse, Exposure Therapy, Genes, Panic Disorder, Polymorphism, Cognitive Behavior Therapy, Treatment Outcomes},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2016-42195-001&site=ehost-live, miguelangelfullana@gmail.com, cgarcia@imim.es},
  database.provider = {EBSCOhost},
  database.name = {psyh},
  ER = {},
  address.author = {Fullana, Miquel A.},
  U1 = {Sponsor: Carlos III Health Institute/Fondo Europeo de Desarrollo Regional. Grant: PI12/00273. Recipients: Fullana, Miquel A., Sponsor: Spanish Ministry of Economy and Competitivity, Spain. Grant: PN 2008-2011. Recipients: No recipient indicated, Sponsor: Carlos III Health Institute/Fondo Europeo de Dessarrollo Regional. Grant: PI1200018. Recipients: No recipient indicated, Sponsor: Centro de Investigación Biomédica en Red-Salud Mental. Recipients: No recipient indicated, Sponsor: Catalan Government, Ministry of the Vice-presidency and of the Economy and Finance, Universities and Research Secretariat, Spain. Grant: SGR 1636. Date: from 2014. Recipients: No recipient indicated},
