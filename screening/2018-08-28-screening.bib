  doi = {10.1007/s00787-012-0367-5},
  U1 = {Sponsor: LSG-Rentray. Recipients: No recipient indicated, Sponsor: Residential and Ambulant Treatment Center for Children and Adolescents, Netherlands. Recipients: No recipient indicated},
