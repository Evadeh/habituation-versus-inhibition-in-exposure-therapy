  doi = {10.1007/978-81-322-2241-5_2},
  ED = {Mehta, Manju, Sagar, Rajesh, Mehta, Manju, (Ed), Sagar, Rajesh, (Ed)},
  title.secundary = {A practical approach to cognitive behaviour therapy for adolescents.},
  recordLog = {PsycINFO},
}

