@article{Fond2013-20180828,
  id.record = {2013-13031-006},
  number.accession = {2013-13031-006},
  author = {Fond, G. and Franc, N.},
  title = {Traiter la phobie spécifique de l’enfant en une séance? Une revue systématique de la littérature. = Treating specific childhood phobia in a single session? A systematic review of the literature.},
  journal = {L'Encéphale: Revue de psychiatrie clinique biologique et thérapeutique},
  JO = {L'Encéphale: Revue de psychiatrie clinique biologique et thérapeutique},
  journal.abbreviated = {Encephale},
