  address.author = {Dyregrov, Atle, Center for Crisis Psychology, Fabrikkgaten 5, 5059, Bergen, Norway},
  doi = {10.1111/j.1475-3588.2005.00384.x},
  recordLog = {PsycINFO},
}

@article{Clarke2015-20180828,
