}

@phdthesis{Franklin2007-20180828,
  id.record = {2007-99010-187},
  number.accession = {2007-99010-187},
  author = {Franklin-Zitzkat, Liese M.},
  title = {The use of flow in the treatment of clinical anxiety.},
  journal = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  JO = {Dissertation Abstracts International: Section B: The Sciences and Engineering},
  year = {2007///},
  volume = {67},
  issue = {11-B},
  page.start = {6735},
  page.end = {6735},
  place.published = {US},
  publisher = {ProQuest Information & Learning},
  number.serial = {0419-4217},
  notes = {Accession Number: 2007-99010-187. Other Journal Title: Dissertation Abstracts International. Partial author list: First Author & Affiliation: Franklin-Zitzkat, Liese M.; U Hartford, US. Release Date: 20070806. Publication Type: Dissertation Abstract (0400). Format Covered: Electronic. Document Type: Dissertation. Dissertation Number: AAI3243154. Language: English. Major Descriptor: Anxiety Disorders; Cognitive Therapy; Self-Esteem. Minor Descriptor: Psychotherapy. Classification: Health & Mental Health Treatment & Prevention (3300). Population: Human (10). Methodology: Empirical Study; Quantitative Study. Page Count: 1.},
  abstract = {Cognitive-behavioral therapy is regarded as a particularly effective approach to treating anxiety disorders, but is limited in that it emphasizes dysfunction and ignores the existence of a conscious self. Flow, a state achieved by an individual when his or her perceived skills are equivalent to the challenges presented by a particular situation, incorporates individuals' strengths and the conscious self into its theory. Flow can be conceptualized as a state of optimal functioning People in a state of flow achieve a sense of complete control, are totally absorbed in their activities, and typically lose all sense of time, place, and self. Subjective well-being and self-esteem have been positively correlated with the amount of time spent in flow, and flow deprivation has been shown to produce anxiety symptoms. Thus, incorporating flow into cognitive-behavioral therapy for anxiety disorders might prove advantageous in that it could (a) potentially correct for some limitations of cognitive-behavioral psychotherapy, (b) provide clinicians with alternate ways of conceptualizing their clients' issues, symptoms, and strengths, and (c) introduce specific strategies to enhance and complement cognitive-behavioral techniques. This manual provides suggested guidelines for integrating flow with cognitive-behavioral therapy in the treatment of anxiety. Flow can be assessed in clinical treatment using the Experience-Sampling Method or less formal measures, and cognitive-behavioral case formulations can be expanded to include flow. Flow can be incorporated into several cognitive-behavioral techniques, including cognitive restructuring, activity monitoring and scheduling, pie charts, distraction, relaxation, imagery techniques, and graded exposure. Outcome can be measured using client-driven or objective measures. The guidelines set forth in the manual have not been applied to clinical cases. Research is needed to refine the methods described and to determine their efficacy. Suggested future clinical applications of flow include using flow in both individual and child and family therapy to treat conditions such as depression and eating disorders. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {clinical anxiety, clinical applications, eating disorders, perceived skills, cognitive behavioral psychotherapy, self esteem, Anxiety Disorders, Cognitive Therapy, Self-Esteem, Psychotherapy},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2007-99010-187&site=ehost-live},
  database.provider = {EBSCOhost},
