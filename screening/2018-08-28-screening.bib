@incollection{Farrell2016a-20180828,
  id.record = {2015-31751-023},
  number.accession = {2015-31751-023},
  author = {Farrell, Lara J. and James, Sophie C. and Maddox, Brenna B. and Griffiths, Donna and White, Susan},
  title = {Treatment of comorbid obsessive-compulsive disorder in youth with ASD: The case of Max.},
  year = {2016///},
  page.start = {337},
  page.end = {355},
  place.published = {Cham, Switzerland},
  publisher = {Springer International Publishing},
  number.serial = {978-3-319-17138-8, 978-3-319-17139-5},
  notes = {Accession Number: 2015-31751-023. Partial author list: First Author & Affiliation: Farrell, Lara J.; School of Applied Psychology and Behavioural Basis of Health, Griffith University, Gold Coast, QLD, Australia. Release Date: 20160222. Correction Date: 20180517. Publication Type: Book (0200), Edited Book (0280). Format Covered: Print. Document Type: Chapter. Book Type: Handbook/Manual. ISBN: 978-3-319-17138-8, Hardcover; 978-3-319-17139-5, Digital (undefined format). Language: English. Major Descriptor: Autism Spectrum Disorders; Cognitive Behavior Therapy; Comorbidity; Exposure Therapy; Obsessive Compulsive Disorder. Minor Descriptor: Parental Involvement. Classification: Psychological & Physical Disorders (3200); Health & Mental Health Treatment & Prevention (3300). Population: Human (10); Male (30). Age Group: Adolescence (13-17 yrs) (200). Intended Audience: Psychology: Professional & Research (PS). Tests & Measures: Social Responsiveness Scale; Pediatric Quality of Life; anxiety disorders interview schedule for children for DSM-IV—parent report; Child Depression Inventory DOI: 10.1037/t19482-000; Family Accommodation Scale DOI: 10.1037/t29858-000; Multidimensional Anxiety Scale for Children DOI: 10.1037/t05050-000; Children's Yale-Brown Obsessive-Compulsive Scale DOI: 10.1037/t42601-000. Methodology: Clinical Case Study. References Available: Y. Page Count: 19.},
  abstract = {This chapter discusses the treatment of comorbid obsessive-compulsive disorder (OCD) in youth with autism spectrum disorder (ASD). The case of Max demonstrated that cognitive behavior therapy (CBT) with exposure & response prevention (ERP) can be effective in treating comorbid OCD in a young person with ASD. While the treatment approach used in this case was intensive, the therapeutic process and outcomes would be very similar if given in standard weekly sessions. This testament produced a clinically significant reduction in OCD, anxiety and depression symptoms, while improvements were also seen in Max's quality of life and overall functioning. Max's parents were also to benefit, being able to enjoy leisurely activities together once more. With Max's distress and demands for accommodation greatly reduced, the family unit was functioning much better at conclusion of therapy. The present case highlights the importance of psychoeducation concerning family accommodation in the treatment process of pediatric OCD, particular when accommodation practices are extreme, which might be a correlate of ASD comorbidity. An important consideration when treating comorbid OCD and ASD is parental involvement in the treatment process. In clinical practice, a clear understanding of the distinctions between ASD and OCD may prevent diagnostic overshadowing, which occurs when impairing obsessions and compulsions are attributed to the diagnosis of ASD and not recognized as OCD symptoms. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
  keywords = {comorbidity, obsessive compulsive disorder, autism spectrum disorder, exposure & response prevention, cognitive behavior therapy, family accommodation, Autism Spectrum Disorders, Cognitive Behavior Therapy, Comorbidity, Exposure Therapy, Obsessive Compulsive Disorder, Parental Involvement},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2015-31751-023&site=ehost-live, ORCID: 0000-0002-6274-3147, , sww@vt.edu},
