@article{Ale2015-20180828,
  id.record = {2015-23936-001},
  number.accession = {2015-23936-001},
  author = {Ale, Chelsea M. and McCarthy, Denis M. and Rothschild, Lilianne M. and Whiteside, Stephen P. H.},
  title = {Components of cognitive behavioral therapy related to outcome in childhood anxiety disorders.},
  journal = {Clinical Child and Family Psychology Review},
  JO = {Clinical Child and Family Psychology Review},
  journal.abbreviated = {Clin Child Fam Psychol Rev},
  year = {2015/09//},
  volume = {18},
  issue = {3},
  page.start = {240},
  page.end = {251},
  place.published = {Germany},
  publisher = {Springer},
  number.serial = {1096-4037, 1573-2827},
  notes = {Accession Number: 2015-23936-001. Partial author list: First Author & Affiliation: Ale, Chelsea M.; Department of Psychiatry and Psychology, Mayo Clinic, Rochester, MN, US. Release Date: 20150601. Correction Date: 20160104. Publication Type: Journal (0100), Peer Reviewed Journal (0110). Format Covered: Electronic. Document Type: Journal Article. Language: English. Major Descriptor: Anxiety Disorders; Cognitive Behavior Therapy; Pediatrics; Treatment Outcomes. Minor Descriptor: Obsessive Compulsive Disorder. Classification: Cognitive Therapy (3311). Population: Human (10). Age Group: Childhood (birth-12 yrs) (100). Tests & Measures: Pediatric Anxiety Rating Scale. Methodology: Meta Analysis. References Available: Y. Page Count: 12. Issue Publication Date: Sep, 2015. Publication History: First Posted Date: May 23, 2015. Copyright Statement: Springer Science+Business Media New York. 2015.},
  abstract = {The present article uses meta-analysis to examine treatment components related to outcome within 35 randomized controlled trials (RCTs) for childhood anxiety disorders (CADs) and eight RCTs for childhood obsessive compulsive disorder (OCD). Examination of the RCTs of cognitive behavioral therapy (CBT) for CADs suggested that adding relaxation and delaying exposures until after the introduction of other anxiety management strategies does not increase the efficacy of exposure-based treatment. In addition, compared to the large effect size (ES) associated with exposure and response prevention (ERP) for OCD (k = 9, mean ES = 1.93), the effect size associated with CBT for CADs (k = 44, mean ES = 0.89) did not differentiate from attention placebo (k = 11, mean ES = 0.55), although it was more effective than waitlist control (k = 24, mean ES = 0.22). Instructively, ERP for OCD involved more exposure initiated earlier and less relaxation than CBT for CADs. In addition, RCTs of ERP were more likely to use clinician-administered measures as opposed to self-report and to be conducted in clinical versus recruited samples. These results suggest that dismantling studies using a gold-standard clinician-rated outcome measure to compare the value of adding anxiety management strategies to exposure will be necessary to increase the efficacy of CBT for CADs to levels achieved by ERP for OCD. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Child, Anxiety disorders, Exposure, Cognitive behavioral therapy, Obsessive compulsive disorder, Meta-analysis, Anxiety Disorders, Cognitive Behavior Therapy, Pediatrics, Treatment Outcomes, Obsessive Compulsive Disorder},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2015-23936-001&site=ehost-live, whiteside.stephen@mayo.edu},
  database.provider = {EBSCOhost},
