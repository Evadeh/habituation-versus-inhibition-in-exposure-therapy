  ED = {Weisz, John R., Kazdin, Alan E., Weisz, John R., (Ed), Kazdin, Alan E., (Ed)},
  title.secundary = {Evidence-based psychotherapies for children and adolescents.},
  recordLog = {PsycINFO},
}

@incollection{Cohen2010-20180828,
  id.record = {2010-09488-019},
  number.accession = {2010-09488-019},
  author = {Cohen, Judith A. and Mannarino, Anthony P. and Deblinger, Esther},
  title = {Trauma-focused cognitive-behavioral therapy for traumatized children.},
  year = {2010///},
