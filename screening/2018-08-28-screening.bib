@misc{Aydin2010a-20180828,
  author = {Aydin A and Tekinsav Sutcu S and Sorias O},
  title = {[Evaluation of the effectiveness of a cognitive-behavioral therapy program for alleviating the symptoms of social anxiety in adolescents].},
